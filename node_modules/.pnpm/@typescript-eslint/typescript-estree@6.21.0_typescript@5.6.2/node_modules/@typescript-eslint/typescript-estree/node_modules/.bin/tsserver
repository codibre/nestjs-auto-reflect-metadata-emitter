#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.6.2/node_modules/typescript/bin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.6.2/node_modules/typescript/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.6.2/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/@typescript-eslint/typescript-estree@6.21.0_typescript@5.6.2/node_modules/@typescript-eslint/typescript-estree/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/@typescript-eslint/typescript-estree@6.21.0_typescript@5.6.2/node_modules/@typescript-eslint/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/@typescript-eslint/typescript-estree@6.21.0_typescript@5.6.2/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/@typescript-eslint/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.6.2/node_modules/typescript/bin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.6.2/node_modules/typescript/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.6.2/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/@typescript-eslint/typescript-estree@6.21.0_typescript@5.6.2/node_modules/@typescript-eslint/typescript-estree/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/@typescript-eslint/typescript-estree@6.21.0_typescript@5.6.2/node_modules/@typescript-eslint/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/@typescript-eslint/typescript-estree@6.21.0_typescript@5.6.2/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/@typescript-eslint/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../typescript@5.6.2/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../../../typescript@5.6.2/node_modules/typescript/bin/tsserver" "$@"
fi
