#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.3.3/node_modules/typescript/bin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.3.3/node_modules/typescript/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.3.3/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/fork-ts-checker-webpack-plugin@9.0.2_typescript@5.3.3+webpack@5.94.0/node_modules/fork-ts-checker-webpack-plugin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/fork-ts-checker-webpack-plugin@9.0.2_typescript@5.3.3+webpack@5.94.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.3.3/node_modules/typescript/bin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.3.3/node_modules/typescript/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/typescript@5.3.3/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/fork-ts-checker-webpack-plugin@9.0.2_typescript@5.3.3+webpack@5.94.0/node_modules/fork-ts-checker-webpack-plugin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/fork-ts-checker-webpack-plugin@9.0.2_typescript@5.3.3+webpack@5.94.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.3.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.3.3/node_modules/typescript/bin/tsserver" "$@"
fi
