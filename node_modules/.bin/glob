#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/glob@10.4.2/node_modules/glob/dist/esm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/glob@10.4.2/node_modules/glob/dist/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/glob@10.4.2/node_modules/glob/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/glob@10.4.2/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/glob/dist/esm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/glob/dist/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/glob/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/glob@10.4.2/node_modules/glob/dist/esm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/glob@10.4.2/node_modules/glob/dist/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/glob@10.4.2/node_modules/glob/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/glob@10.4.2/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/glob/dist/esm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/glob/dist/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/glob/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../glob/dist/esm/bin.mjs" "$@"
fi
