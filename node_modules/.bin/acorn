#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/acorn@8.12.1/node_modules/acorn/bin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/acorn@8.12.1/node_modules/acorn/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/acorn@8.12.1/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/acorn/bin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/acorn/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/acorn@8.12.1/node_modules/acorn/bin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/acorn@8.12.1/node_modules/acorn/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/acorn@8.12.1/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules:/home/thiago/projects/personal/codibre/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/acorn/bin/node_modules:/home/thiago/projects/personal/codibre/nestjs-auto-reflect-metadata-emitter/node_modules/acorn/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
